package com.abm.mainet.common.master.dao;

import java.util.List;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.PagingAndSortingRepository;
import org.springframework.data.repository.query.Param;
import com.abm.mainet.common.domain.TbCustbanksEntity;


/**
 * Repository : TbCustbanks.
 */
public interface TbCustbanksJpaRepository extends PagingAndSortingRepository<TbCustbanksEntity, Long> {

	@Query("select tbCustbanksEntity from TbCustbanksEntity tbCustbanksEntity where tbCustbanksEntity.tbCustbanksMas.cmBankid = :cmBankid")
	List<TbCustbanksEntity> findAllByCmBankId(@Param("cmBankid") Long cmBankid);
}
